{
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true,
    "jest": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-console": "off"
  },
  "parser": "babel-eslint"
}
