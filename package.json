{
  "name": "gullwing",
  "version": "1.1.1",
  "description": "dynamic templates with optional transformation",
  "main": "dist/gullwing.cjs.js",
  "module": "dist/gullwing.esm.js",
  "browser": "dist/gullwing.umd.js",
  "scripts": {
    "build": "NODE_ENV=build rollup -c",
    "test": "NODE_ENV=test jest",
    "watch:test": "NODE_ENV=test jest --watch",
    "prepublish": "npm run build"
  },
  "files": ["dist"],
  "keywords": [
    "template",
