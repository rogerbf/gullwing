{
  "name": "gullwing",
  "version": "2.0.0",
  "description": "a minimal and adaptable i18n/g11n library",
  "main": "build/gullwing.js",
  "unpkg": "build/gullwing.umd.js",
  "module": "build/gullwing.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf build coverage .cache dist package gullwing-*.tgz",
    "prepare": "npm run clean && npm test",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:development": "NODE_ENV=development jest",
    "test:watch:development": "npm run test:development -- --watch",
    "test:coverage:development": "npm run test:development -- --coverage"
  },
  "keywords": [
    "template",
    "templates",
    "i18n",
    "g11n",
    "globalization",
    "internationalization",
    "localization",
    "localize"
  ],
  "author": "rogerbf <roger@bergfa.lk> (http://bergfa.lk/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "eslint": "5.10.0",
    "jest": "23.6.0",
    "rimraf": "2.6.2",
    "rollup": "0.67.4",
    "rollup-plugin-babel": "4.1.0",
    "rollup-plugin-commonjs": "9.2.0",
    "rollup-plugin-node-resolve": "4.0.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-terser": "3.0.0"
  },
  "dependencies": {},
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerbf/gullwing.git"
  },
  "bugs": {
    "url": "https://github.com/rogerbf/gullwing/issues"
  },
  "homepage": "https://github.com/rogerbf/gullwing#readme"
}
