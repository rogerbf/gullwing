{
  "name": "gullwing",
  "version": "4.0.0",
  "description": "A minimal string interpolation library",
  "main": "index.js",
  "module": "source/index.js",
  "scripts": {
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.js'",
    "pretest": "npm run format && npm run lint",
    "test": "ava",
    "coverage": "c8 --reporter=html --reporter=text --exclude tests npm run test",
    "develop": "ava --watch",
    "prepare": "npm run test"
  },
  "keywords": [
    "template",
    "templates",
    "i18n",
    "g11n",
    "globalization",
    "internationalization",
    "localization",
    "localize",
    "string",
    "interpolation"
  ],
  "author": "rogerbf <roger@bergfa.lk> (http://bergfa.lk/)",
  "license": "MIT",
  "devDependencies": {
    "ava": "3.13.0",
    "c8": "7.3.4",
    "eslint": "7.11.0",
    "prettier": "2.1.2",
    "sinon": "9.2.0"
  },
  "dependencies": {
    "call-tree": "3.0.0",
    "esm": "3.2.25"
  },
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerbf/gullwing.git"
  },
  "bugs": {
    "url": "https://github.com/rogerbf/gullwing/issues"
  },
  "homepage": "https://github.com/rogerbf/gullwing#readme",
  "ava": {
    "require": [
      "esm"
    ]
  }
}
