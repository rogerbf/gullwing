{
  "name": "gullwing",
  "version": "2.0.1",
  "description": "a minimal and adaptable i18n/g11n library",
  "main": "build/gullwing.js",
  "unpkg": "build/gullwing.umd.js",
  "module": "build/gullwing.es.js",
  "files": [
    "build"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf build coverage .cache dist package gullwing-*.tgz",
    "prepare": "npm run clean && npm test",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:development": "NODE_ENV=development jest",
    "test:watch:development": "npm run test:development -- --watch",
    "test:coverage:development": "npm run test:development -- --coverage"
  },
  "keywords": [
    "template",
    "templates",
    "i18n",
    "g11n",
    "globalization",
    "internationalization",
    "localization",
    "localize"
  ],
  "author": "rogerbf <roger@bergfa.lk> (http://bergfa.lk/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/plugin-syntax-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "24.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "24.8.0",
    "prettier": "^1.17.1",
    "rimraf": "2.6.3",
    "rollup": "1.13.0",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.1",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-terser": "5.0.0"
  },
  "dependencies": {},
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerbf/gullwing.git"
  },
  "bugs": {
    "url": "https://github.com/rogerbf/gullwing/issues"
  },
  "homepage": "https://github.com/rogerbf/gullwing#readme"
}
